{
    "ConnectionStrings": {
        "Postgres": "<from-user-secrets>"
    },
    "TelegramConfiguration": {
        "Token": "<from-user-secrets>>",
        "HostAddress": "<from-ngrok>",
        "Route": "/bot",
        "Secret": "<from-user-secrets>"
    },
    "Serilog": {
        "Using": [
            "Serilog.Sinks.Console",
            "Serilog.Sinks.Seq"
        ],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        },
        "WriteTo": [
            {
                "Name": "Console"
            },
            {
                "Name": "Seq",
                "Args": {
                    "serverUrl": "http://localhost:5341"
                }
            }
        ],
        "Enrich": [
            "WithEnvironmentName"
        ]
    }
}